{
  "framework": "chakra",
  "libraries": [
    {
      "name": "ui",
      "path": "libs/ui/src",
      "storyTitlePrefix": "UI",
      "importAlias": "@forgekit-landing/ui"
    }
  ],
  "componentPatterns": [
    "**/*.tsx",
    "!**/*.stories.tsx",
    "!**/*.test.tsx",
    "!**/index.ts",
    "!**/index.tsx",
    "!**/theme/**"
  ],
  "generateTests": false,
  "generateDocs": false
}
